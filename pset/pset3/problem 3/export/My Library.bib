  shorttitle = {Particulate {{Matter Air Pollution Exposure}} and {{Heart Disease Mortality Risks}} by {{Race}} and {{Ethnicity}} in the {{United States}}},
  author = {Parker, Jennifer D. and Kravets, Nataliya and Vaidyanathan, Ambarish},

@article{weichenthal_how_2022,
  title = {How Low Can You Go? {{Air}} Pollution Affects Mortality at Very Low Levels},
  shorttitle = {How Low Can You Go?},
  author = {Weichenthal, Scott and Pinault, Lauren and Christidis, Tanya and Burnett, Richard T. and Brook, Jeffrey R. and Chu, Yen and Crouse, Dan L. and Erickson, Anders C. and Hystad, Perry and Li, Chi and Martin, Randall V. and Meng, Jun and Pappin, Amanda J. and Tjepkema, Michael and {van Donkelaar}, Aaron and Weagle, Crystal L. and Brauer, Michael},
  year = {2022},
  month = sep,
  journal = {Science Advances},
  volume = {8},
  number = {39},
  pages = {eabo3381},
  publisher = {{American Association for the Advancement of Science}},
  doi = {10.1126/sciadv.abo3381},
  abstract = {The World Health Organization (WHO) recently released new guidelines for outdoor fine particulate air pollution (PM2.5) recommending an annual average concentration of 5 {$\mu$}g/m3. Yet, our understanding of the concentration-response relationship between outdoor PM2.5 and mortality in this range of near-background concentrations remains incomplete. To address this uncertainty, we conducted a population-based cohort study of 7.1 million adults in one of the world's lowest exposure environments. Our findings reveal a supralinear concentration-response relationship between outdoor PM2.5 and mortality at very low ({$<$}5 {$\mu$}g/m3) concentrations. Our updated global concentration-response function incorporating this new information suggests an additional 1.5 million deaths globally attributable to outdoor PM2.5 annually compared to previous estimates. The global health benefits of meeting the new WHO guideline for outdoor PM2.5 are greater than previously assumed and indicate a need for continued reductions in outdoor air pollution around the world.}
}

